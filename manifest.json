{
  "manifest_version": 2,
  "name": "Ticket Prettifier",
  "version": "1.0",

  "description": "Mejora el diseno de los tickets",

  "icons": {
    "16": "icons/icon-16.svg",
    "48": "icons/icon-48.svg",
    "128": "icons/icon-128.svg"
  },

  "content_scripts": [
    {
      "matches": ["*://ticketmaker.azurewebsites.net/*"],
      "js": ["ticketprettifier.js"]
    }
  ]
}
